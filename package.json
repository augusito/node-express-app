{
  "name": "node-express-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env DEBUG=express-sequelize node bin/app",
    "dev": "cross-env DEBUG=express-sequelize nodemon bin/app",
